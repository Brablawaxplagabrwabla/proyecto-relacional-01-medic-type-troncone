  <script>
    $(document).ready(function(){
        $(".selectP").click(function(){
            $(".menuP").removeClass("hide");
            $(".menuPM").addClass("hide");
        });

        $(".selectM").click(function(){
            $(".menuM").removeClass("hide");
            $(".menuPM").addClass("hide");
        });
    });
  </script>


