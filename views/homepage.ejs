<!-- SELECCIONAR MEDICO/PACIENTE -->
<div class="menuPM container">
  <div class="section">
    <div class="row">
      <div class="col l3"></div>
      <div class="col l6">
        <div class="row center-align">
          <a class="selectP" href="#">
          <div style="padding: 0px;" class="col l6"><img class="responsive-img" src="https://i.imgur.com/UAhDdjf.png" alt="">
          <div style="width: 100%; box-shadow: none; border-radius: 0px;" class="prueba123 btn-p waves-effect waves-light btn teal accent-4">INGRESO - PACIENTE</div>
          </div>
          </a>
          <a class="selectM" href="#">
          <div style="padding: 0px;" class="col l6"><img class="responsive-img" src="https://i.imgur.com/rMdxoM9.png" alt="">
