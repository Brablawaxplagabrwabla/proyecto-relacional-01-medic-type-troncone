<html lang="en">
<head>

</head>
<body>


<!-- LISTA DE PACIENTES -->
<div class="container">
  <div class="section">
    <div class="row">
      <br><br>
      <h4 class="col s12 light center flow-text"> Medicamentos con mismo compuesto activo: <%= compactivo %> </h4>

      <table class="highlight centered">

        <thead>
        <tr>
          <th class="teal-text text-accent-4">Medicamento:</th>
          <th></th>
        </tr>
        </thead>
        <tbody>
        <% _.forEach(medicamento, function(medicamento) { %>
        <tr data-id="<%= medicamento.idMedicamento %>" data-model="alergia" >
          <td><%= medicamento.NomComercial %></td>
          <td>
            <a href="/CompActivo/show/<%= medicamento.idMedicamento %>" class="material-icons teal-text text-accent-4">remove_red_eye</a>
          </td>
        </tr>
        <% }) %>
        </tbody>
      </table>
    </div>
  </div>
  <br><br>
</div>
<!-- FIN LISTA DE PACIENTES -->


</body>
</html>
