<form action="/Medicamento/update" method="POST">
  <head>

  </head>
  <body>

  <br>




  <div class="container">

    <div class="row">
      <form class="col s12">
        <div class="row">

          <div class="col s12 m12 l12">
            <h4 class="flow-text center teal-text text-accent-4">Editar Medicamento: </h4>
          </div>

          <div class="input-field col s12 m6 l6">
            <input name="nombreComercial" id="icon_telephone" type="text" class="validate">
            <label for="icon_telephone">Nombre Comercial</label>
          </div>

          <div class="input-field col s12 m6 l6">
            <input name="id" value="<%= medicamento.id %>" id="icon_telephone" type="hidden" class="validate">
          </div>

          <div class="input-field col s12 m6 l6">
            <input name="efectosSec" id="icon_telephone" type="text" class="validate">
            <label for="icon_telephone">Efectos Secundarios</label>
          </div>

          <div class="input-field col s12 m6 l12">
            <input name="contraindicaciones" type="text" class="validate">
            <label for="icon_telephone">Contraindicaciones</label>
          </div>

          <center>
            <input class="btn waves-effect waves-light" type="submit" name="action" value="Enviar"></input>
          </center>

        </div>
      </form>
    </div>


  </div>


  <br><br>




  </body>
  </html>
</form>
